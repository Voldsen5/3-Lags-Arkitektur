        <Label x:Name="YearsEmployed" Content="" HorizontalAlignment="Left" Height="47" Margin="80,170,0,0" VerticalAlignment="Top" Width="75"/>
        <Button x:Name="Add" Content="Add" HorizontalAlignment="Left" Height="28" Margin="532,68,0,0" VerticalAlignment="Top" Width="81" Click="Add_Click"/>
        <TextBox x:Name="NameToAdd" HorizontalAlignment="Left" Height="23" Margin="554,120,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="59"/>
